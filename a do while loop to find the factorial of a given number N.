import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int i=1, fact=1;
        do{
          fact=fact*i;
          i++;
        }while(i<=n);
        System.out.println("factorial of "+n+"="+fact);
    }
}
